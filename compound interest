#include<stdio.h>
#include<math.h>
void main()
{
    double p,t,r,amt,ci,n;
    printf("Enter the princple amount:");
    scanf("%lf",&p);
    printf("\nEnter the time period:");
    scanf("%lf",&t);
    printf("\nEnter the time compund:");
    scanf("%lf",&n);
    printf("\nEnter the rate of interst:");
    scanf("%lf",&r);
    amt=p*pow(1+(r/100/n),(n*t));
    ci=amt-p;
    printf("\nThe Compound interst of the princle amount is:%.1lf",ci);
}







