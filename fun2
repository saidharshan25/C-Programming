#include <stdio.h>
void add(int a, int b);

void main()
{
    int num1, num2;
    printf("Enter the first number: ");
    scanf("%d", &num1);
    printf("Enter the second number: ");
    scanf("%d", &num2);
    add(num1, num2);
}
void add(int a, int b) 
{
    int sum = a + b;
    printf("The sum of %d and %d is: %d\n", a,
    b, sum);
}