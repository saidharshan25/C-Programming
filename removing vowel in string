#include <stdio.h>

void main() {
    int i=0;
    char a[100];
    printf("Enter a string :");
    scanf("%s",a);
    while(a[i]!='\0')
    {
        if(a[i]!='A' && a[i]!='E' && a[i]!='I'&&a[i]!='O'&&a[i]!='U'&&a[i]!='a'&&a[i]!='e'&&a[i]!='i'&&a[i]!='o'&&a[i]!='u')
        {
            printf("\n%c is a consonents",a[i]);
        }
        i++;
    }
}
