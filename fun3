#include <stdio.h>
int add();

void main() 
{
    int sum;
    sum = add();
    printf("The sum of the two numbers is: 
    %d\n", sum);
}
int add() 
{
    int num1, num2;
    printf("Enter the first number: ");
    scanf("%d", &num1);
    printf("Enter the second number: ");
    scanf("%d", &num2);
    return num1 + num2;
}