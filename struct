#include <stdio.h>
struct student{
    int id;
     char name[20];
};
void main() {
    printf("Enter students details: \n");
    struct student s[3];
    for(int i=0;i<3;i++)
    {
        scanf("%d",&s[i].id);
    scanf("%s",&s[i].name);
    }
    for(int j=0;j<3;j++)
    {
        printf("ID: %d\n",s[j].id);
        printf("Name: %s\n",s[j].name);
    }
}