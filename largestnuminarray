#include<stdio.h>
void main()
{
    int n;
    printf("Enter the number of the elements in the array:");
    scanf("%d",&n);
    int arr[n];
    printf("enter the the element of the array:\n");
    for (int i=0;i<n;i++)
    {
        scanf("%d",&arr[i]);
    }
    int largest=0;
    for (int i=1;i<n;i++)
        {
            if (arr[i]>largest)
            {
                largest=arr[i];
            }  
        }
    printf("The largest element in the array is: %d \n",largest);
    }
