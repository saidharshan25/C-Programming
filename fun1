#include <stdio.h>
int add(int a, int b);

void main() 
{
    int num1, num2,sum;
    printf("Enter the first number: ");
    scanf("%d", &num1);

    printf("Enter the second number: ");
    scanf("%d", &num2);
    sum = add(num1, num2)
    printf("The sum of %d and %d is: %d\n",  
    num1, num2,sum);
}
int addNumbers(int a, int b) 
{
    return a + b;
}