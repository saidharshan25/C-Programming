#include<stdio.h>
 void main()
 { 
char ch;
 printf("Enter any character:"); 
 scanf("%c",&ch);
 switch(ch) 
{
 case 'a':
 case'A': 
   printf("\nvowel"); 
   break;
 case 'e':
 case 'E':
   printf("\nvowel"); 
   break;
 case 'i':
 case'I':
   printf("\nvowel");
   break;
 case 'o':
 case'O': 
   printf("\nvowel");
   break; 
 case'u':
 case'U': 
   printf("\nvowel"); 
   break;
 default:
   printf("\nconsonant"); 
} 
}