#include<stdio.h>
 void main()
 { 
char character;
 printf("Enter any character:"); scanf("%c",&character);
 switch(character) 
{
 case 'a'||'A': 
   printf("\nvowle"); 
   break;
 case 'e'||'E':
   printf("\nvowel"); 
   break;
 case 'i'||'I':
   printf("\nvowel");
   break;
 case 'o'||'O': 
   printf("\nvowel");
   break; 
 case'u'||'U': 
   printf("\nvowel"); 
   break;
 default:
   printf("\nconsonant"); 
} 
}